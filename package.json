{
  "name": "playground-frontend",
  "version": "2.1.1",
  "description": "area di test e verifiche veloci",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "start:serve": "serve -s build",
    "build": "react-scripts build",
    "test": "jest",
    "test:react": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "javascript",
    "zustand",
    "playground",
    "todo list",
    "spinner"
  ],
  "author": "AleDeP10",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "identity-obj-proxy": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.2"
  }
}